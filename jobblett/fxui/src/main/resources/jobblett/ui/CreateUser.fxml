<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<Pane prefHeight="640.0" prefWidth="360.0" stylesheets="@layout.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jobblett.ui.CreateUserController">
    <Text layoutX="67.0" layoutY="169.0" text="Username:">
        <font>
            <Font name="Arial" size="13.0" />
        </font>
    </Text>
    <Text layoutX="67.0" layoutY="222.0" text="Password:" />
    <Text layoutX="54.0" layoutY="277.0" text="Given name:" />
    <Text layoutX="54.0" layoutY="334.0" text="Family name:" />
    <TextField fx:id="username" layoutX="146.0" layoutY="151.0" onAction="#createAccount" />
    <PasswordField fx:id="password" layoutX="146.0" layoutY="204.0" onAction="#createAccount" />
    <TextField fx:id="givenName" layoutX="146.0" layoutY="259.0" onAction="#createAccount" />
    <TextField fx:id="familyName" layoutX="146.0" layoutY="316.0" onAction="#createAccount" />
    <Button fx:id="createAccountButton" layoutX="177.0" layoutY="371.0" onAction="#createAccount" text="Create account" />
    <Text fx:id="errorMessage" layoutX="85" layoutY="500.0" wrappingWidth="250" />
    <Button fx:id="goBackButton" layoutX="35.0" layoutY="576.0" onAction="#goToLogIn" text="Back" />
</Pane>
